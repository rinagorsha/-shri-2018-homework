/* HTML5 display-role reset for older browsers */
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
header,
main,
hgroup,
menu,
nav,
section,
summary
  display block

table
  border-spacing 0
  border-collapse collapse

input,
textarea,
button,
select,
option
  border-radius 0
  box-shadow none
  -webkit-tap-highlight-color transparent
